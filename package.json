{
  "version": "0.0.4",
  "name": "@intervene/backend",
  "main": "lib/main/index.js",
  "typings": "lib/main/index.d.ts",
  "module": "lib/module/index.js",
  "files": [
    "lib/main",
    "lib/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "lib/main/"
      },
      "compile": false
    },
    "files": [
      "!lib/module/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intervenehq/backend"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typegen": "bun run openapi-typescript /Users/sudhanshugautam/workspace/intervene-landing/docs/openapi.json -o ./src/types/schema.ts",
    "build": "npm run build:main && npm run build:module",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "dev": "tsc -p tsconfig.json -w",
    "dev:module": "tsc -p tsconfig.module.json -w"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "bun-types": "latest",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "type-fest": "^4.8.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "ky": "^1.1.3",
    "openapi-typescript": "^6.7.2"
  }
}
