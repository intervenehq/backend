{
  "version": "0.0.12",
  "name": "@intervene/backend",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.modern.js",
  "umd:main": "./dist/index.umd.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/intervenehq/backend"
  },
  "files": [
    "dist/*",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typegen": "bun run openapi-typescript https://intervene.run/openapi.json -o ./src/types/schema.ts",
    "build": "microbundle --tsconfig ./tsconfig.json  --format modern,cjs,umd  --output dist/index.js",
    "dev": "microbundle watch --tsconfig ./tsconfig.json  --format modern,cjs,umd  --output dist/index.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "bun-types": "latest",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "microbundle": "^0.15.1",
    "openapi-typescript": "^6.7.2",
    "prettier": "^3.0.3",
    "type-fest": "^4.8.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "ky": "^1.1.3"
  }
}
